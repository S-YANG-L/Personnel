	var frequency = getQueryString("frequency");//巡检次数
	var facilityId = getQueryString("facilityId");//设备id
	var endNum = getQueryString("endNum");//完成次数
	/*var IS_FINISH = '0';
	if(frequency-endNum<=0){
		IS_FINISH = '1';
	}*/
