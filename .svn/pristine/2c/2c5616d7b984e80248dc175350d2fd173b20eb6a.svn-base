			summer.openWin({
				"id":"engineering_management"+t,
		        "url" : "html/engineering_management.html"
   				 });
   				//summer.closeWin({});
   				
		}else{
			UM.toast({
			  	  text: response.data.state
				});
		}
	});
	//imagePaths为数组
	for ( i = 0; i < imgPathes.length; i++) {
		var imgPathWS = imgPathes[i].imgPath;
		//多文件上传，5秒超时
		summer.multiUpload({
			fileArray : [{//文件列表，数组
